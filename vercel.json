{
	"version": 2,
	"name": "comments-users-auth",
	"builds": [{ "src": "src/main.ts", "use": "@vercel/node" }],
	"routes": [{ "src": "/(.*)", "dest": "src/main.ts" }],
	"functions": {
		"prisma-generate": {
			"runtime": "nodejs18",
			"memory": 1024,
			"maxDuration": 10,
			"includeFiles": "prisma/schema.prisma"
		}
	}
}
